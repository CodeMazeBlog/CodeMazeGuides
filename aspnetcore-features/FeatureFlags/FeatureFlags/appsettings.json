{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "AllowedHosts": "*",
    "FeatureManagement": {
        "ShowForecast": true,
        "ShowForecastPercentage": {
            "EnabledFor": [
                {
                    "Name": "Percentage",
                    "Parameters": {
                        "Value":  50
                    }
                }
            ]
        }
    }
}
