{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FixedOptions": {
    "PermitLimit": 20,
    "Window": 1,
    "QueueLimit": 0
  },
  "SlidingWindowOptions": {
    "PermitLimit": 10,
    "Window": 1,
    "SegmentsPerWindow": 10,
    "QueueLimit": 0
  },
  "TokenBucketOptions": {
    "TokenLimit": 10,
    "ReplenishmentPeriod": 1,
    "TokensPerPeriod": 10,
    "AutoReplenishment": true,
    "QueueLimit": 0
  },
  "ConcurrencyOptions": {
    "PermitLimit": 1,
    "QueueLimit": 0
  },
  "AuthorizedOptions": {
    "PermitLimit": 10,
    "Window": 1,
    "QueueLimit": 0
  },
  "UnauthorizedOptions": {
    "PermitLimit": 10,
    "Window": 1,
    "QueueLimit": 0
  },
  "ChainedFirstOptions": {
    "PermitLimit": 4,
    "Window": 2
  },
  "ChainedSecondOptions": {
    "PermitLimit": 20,
    "Window": 30
  }
}
